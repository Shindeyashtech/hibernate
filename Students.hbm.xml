<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

<class name="Hiber" table="students" >

<id name ="stud_id" type="int">
<column name="Stud_id"></column>
<generator class="assigned"></generator>
</id>

<property name="stud_name" type="String">
<column name="Stud_name"></column>
</property>

<property name="stud_Address" type="String">
<column name="Stud_address"></column>
</property>

<property name=" stud_DOB" type="int">
<column name="DOB"></column>
</property>

<property name="stud_Div" type="String">
<column name="Stud_Div"></column>
</property>



</class>
</hibernate-mapping>